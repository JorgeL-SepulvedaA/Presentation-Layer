@page
@model PresentationLayer_App.Pages.GreetingsModel
@{
    ViewData["Title"] = "Hello, World!";
}
<fieldset class="container">
    <div class="container" align="justify">
        <h1 class="display-1 text-center text-info">Hello, world!</h1>

        <hr />

        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="https://res.cloudinary.com/practicaldev/image/fetch/s--10ArqA9X--/c_imagga_scale,f_auto,fl_progressive,h_900,q_auto,w_1600/https://dev-to-uploads.s3.amazonaws.com/i/z6pkbof42d5ljfxtox3p.png" style="border-radius: 2px;" alt="Hello, World!">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://www.wallpapertip.com/wmimgs/225-2257906_hello-world-wallpaper.png" style="border-radius: 2px;" alt="C'mon, say it!'">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://i-moha.com/wp-content/uploads/2020/06/hello_world_new_black.png" style="border-radius: 2px;" alt="Louder!">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</fieldset>